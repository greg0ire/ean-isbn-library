{
  "name": "rebuy/ean-isbn-library",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": ["ean", "isbn", "validation", "conversion", "parser", "parsing"],
  "homepage": "https://github.com/rebuy-de/ean-isbn-library",
  "authors": [
    {
      "name": "reBuy reCommerce GmbH",
      "homepage": "https://github.com/rebuy-de/ean-isbn-library/contributors"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.dc1.rebuy",
      "options": {
        "ssl": {
          "allow_self_signed": true
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "Rebuy\\EanIsbn\\": ["src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rebuy\\EanIsbn\\Tests\\": ["tests/"]
    }
  },
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "phpdocumentor/phpdocumentor": "2.*",
    "icomefromthenet/reverse-regex" : "dev-master"
  }
}
